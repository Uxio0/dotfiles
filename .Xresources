!urxvt*inheritPixmap:	true
urxvt*shading:			10


!#include "colors_default"
#include "colors_solarized"
URxvt.intensityStyles: false

!*background: rgb:1a/1a/1a
!*foreground: rgb:aa/aa/aa
!*color0:     rgb:00/00/00
!*color8:     rgb:66/66/66
!*color1:     rgb:9e/18/28
!*color9:     rgb:bc/57/66
!*color2:     rgb:00/88/00
!*color10:    rgb:61/a1/71
!*color3:     rgb:d2/bb/4b
!*color11:    rgb:e7/db/52
!*color4:     rgb:41/41/71
!*color12:    rgb:50/85/af
!*color5:     rgb:96/3c/59
!*color13:    rgb:a9/7a/99
!*color6:     rgb:41/81/79
!*color14:    rgb:6b/a4/a4
!*color7:     rgb:be/be/be
!*color15:    rgb:ff/ff/ff

! Fix font space
URxvt*letterSpace: 0

!urxvt*transparent: false
!urxvt*shading: 50
!urxvt*fading: 50
*font: -*-terminus-medium-r-*-*-14-*-*-*-*-*-*-*
urxvt*font: -*-terminus-medium-r-*-*-14-*-*-*-*-*-*-*,-*-stlarch-medium-r-*-*-10-*-*-*-*-*-*-*,-*-symbola-medium-*-*-*-17-*-*-*-*-*-*-*
!urxvt*font: xft:Inconsolata:size=13:antialias=true:hinting=true
!urxvt*italicFont: xft:Inconsolata:size=13:antialias=true:hinting=true
!urxvt.boldFont: xft:inconsolata:pixelsize=13
!urxvt.bolditalicFont: xft:inconsolata:pixelsize=13
*boldFont:			-*-terminus-medium-r-*-*-14-*-*-*-*-*-*-*
*italicFont:		-*-terminus-medium-r-*-*-14-*-*-*-*-*-*-*
*bolditalicFont:	-*-terminus-medium-r-*-*-14-*-*-*-*-*-*-*
!urxvt*italicFont:		xft:Terminus
!urxvt*bolditalicFont:	xft:Terminus
!urxvt*font: xft:Bitstream\ Vera\ Sans\ Mono:style=roman:pixelsize=13
urxvt*scrollBar: false
urxvt*iso14755 : false

!Scroll Buffer
urxvt.saveLines:10000

!Extensions
!URxvt.perl-ext-common:  default,matcher
URxvt.perl-ext-common:  default,clipboard
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.url-launcher:     /usr/bin/firefox
URxvt.matcher.button:   1
URxvt.colorUL: #4682B4
